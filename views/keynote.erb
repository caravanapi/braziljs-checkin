<div class="content keynote-page">
  <header>
  <a href="/" class="button back ajax">Voltar</a>
  <h1><%= @keynote.author %></h1>
  </header>

  <div id="main">
    <section id="checkin-now">
    <figure>
      <img src="/img/photos/<%= @keynote.author.parameterize %>.jpg" width="273" height="203" alt="<%= @keynote.author %>">
			<figcaption><%= @keynote.title %></figcaption>
    </figure>
    <% if Keynote.current.first.id == @keynote.id %>
      <a href="/checkin/<%= @keynote.id %>" class="button ajax">Checkin</a>
    <%end%>
    </section>

    <section id="rate-it">
    <ul>
      <li><a class="button ajax" href="/like/<%= @keynote.id %>"><span class="icon-up"><big><%= @keynote.likes %></big></span><strong>curti, macho</strong></a></li>
      <li><a class="button ajax" href="/unlike/<%= @keynote.id %>"><span class="icon-down"><big><%= @keynote.unlikes %></big></span><strong>arre Ã©gua</strong></a></li>
    </ul>
    </section>
  </div>
</div> <!-- end of #container -->
